{
  "name": "mccall_fitbit",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@fitbit/sdk": "~4.1.0",
    "@fitbit/sdk-cli": "^1.7.1",
    "npm-watch": "^0.6.0"
  },
  "fitbit": {
    "appUUID": "46608730-b21f-4c96-b8dd-942e30a1bdef",
    "appType": "app",
    "appDisplayName": "McCall Fitness",
    "iconFile": "resources/icon.png",
    "wipeColor": "#ffffff",
    "requestedPermissions": [
      "access_heart_rate"
    ],
    "buildTargets": [
      "higgs",
      "meson",
      "gemini",
      "mira"
    ],
    "i18n": {},
    "defaultLanguage": "en-US"
  },
  "scripts": {
    "build": "fitbit-build",
    "debug": "fitbit",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "app",
        "resources"
      ],
      "extensions": "js,gui,css"
    }
  }
}
